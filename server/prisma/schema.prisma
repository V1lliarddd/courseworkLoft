generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Categories {
    id            Int             @id @default(autoincrement())
    title         String
    subcategories SubCategories[]
}

model SubCategories {
    id           Int         @id @default(autoincrement())
    title        String
    Categories   Categories? @relation(fields: [categoriesId], references: [id])
    categoriesId Int?
}

model User {
    id           Int    @id @default(autoincrement())
    login        String
    password     String
    name         String
    phone_number String
}
